<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 新闻的Mapper接口Mapping映射 --> 

<!--
    author : 张梓琪
    version: 1.0
    系统基础信息的社区活动的MAPPER.XML
  -->
<mapper namespace="com.neusoft.managerment.communityinfo.mapper.ICommunityMapper">
  
  <cache/>
  <resultMap id="ActResultMap" type="Active">
     <id property="activeno" column="ACTIVENO"/>
      <result property="activetype" column="ACTIVETYPE"/>
      <result property="activetime" column="ACTIVEPLACE"/>
      <result property="activecontent" column="ACTIVECONENT"/> 
  </resultMap>
  
  <insert id="create"  parameterType="Active" flushCache="true">
   insert into community (ACTIVENO,ACTIVETYPE,ACTIVETIME,ACTIVECONTENT)
   values (#{activeno},#{activeplace},#{activetime},#{activecontent})
  
  </insert>
  
  <update id="update" flushCache="true">
   update community set ACTIVETIME=#{activetime},ACTIVESTYPE=#{activetype},ACTIVECONTENT=#{activecontent}
   where ACTIVENO=#{activeno}
  </update>
  
  <delete id="delete" flushCache="true">
   delete from community where  ACTIVENO=#{activeno}
  </delete>
  
  <select id="selectListByAll" resultMap="ActResultMap">
   select * from community
  
  </select>
  <select id="selectListType" resultMap="ActResultMap">
   select * from community where ACTIVESTYPE=#{activetype}
  
  </select>
  
  </mapper>